	<!-- page -->
	<tag>
		<description>The ESUP-Portail page tag.</description>
		<name>page</name>
		<tag-class>org.esupportail.commons.web.jsf.tags.PageTag</tag-class>
		<body-content>JSP</body-content>
		<attribute>
			<description>The selected menu item, optional.</description>
			<name>menuItem</name>
			<deferred-value />
		</attribute>
		<attribute>
			<description>
				The locale of the page, optional (see TagsConfigurator).
			</description>
			<name>locale</name>
			<required>false</required>
			<rtexprvalue>false</rtexprvalue>
			<deferred-value>
				<type>java.util.Locale</type>
			</deferred-value>
		</attribute>
		<attribute>
			<description>
				The title of the page, optional (see TagsConfigurator).
			</description>
			<name>title</name>
			<deferred-value />
		</attribute>
		<attribute>
			<description>
				The name of the variable in which strings will be
				loaded.
			</description>
			<name>stringsVar</name>
			<deferred-value />
		</attribute>
		<attribute>
			<description>
				The string to display at the end of the page, optional
				(see TagsConfigurator).
			</description>
			<name>footer</name>
			<deferred-value />
		</attribute>
		<attribute>
			<description>
				If set to false, a message is printed to tell the user
				that he is not allowed to view the page. Defaults to
				true.
			</description>
			<name>authorized</name>
			<required>false</required>
			<rtexprvalue>false</rtexprvalue>
			<deferred-value>
				<type>java.lang.Boolean</type>
			</deferred-value>
		</attribute>
		<attribute>
			<description>
				If set to not null, the ID of the download to redirect
				to.
			</description>
			<name>downloadId</name>
			<required>false</required>
			<rtexprvalue>false</rtexprvalue>
			<deferred-value>
				<type>java.lang.String</type>
			</deferred-value>
		</attribute>
	</tag>
