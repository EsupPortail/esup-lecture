<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE faces-config PUBLIC "-//Sun Microsystems, Inc.//DTD JavaServer Faces Config 1.1//EN"
                              "http://java.sun.com/dtd/web-facesconfig_1_1.dtd">
<!-- 
	/properties/jsf/navigation-rules.xml
	The configuration file of Java Server Faces that gives 
	the navigation rules between views.
	This file is referenced by /webapp/WEB-INF/web.xml. 
	It should not be modified but by the developers. 
-->

<faces-config>

	<navigation-rule>
		<display-name>index.jsp -> welcome.jsp</display-name>
		<from-view-id>/index.jsp</from-view-id>
		<navigation-case>
			<from-outcome>welcome</from-outcome>
			<to-view-id>/stylesheets/welcome.jsp</to-view-id>
			<redirect />
		</navigation-case>
	</navigation-rule>

	<navigation-rule>
		<display-name>administrators</display-name>
		<from-view-id>/stylesheets/administrators.jsp</from-view-id>
		<navigation-case>
			<from-outcome>addAdmin</from-outcome>
			<to-view-id>/stylesheets/administratorAdd.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>deleteAdmin</from-outcome>
			<to-view-id>
				/stylesheets/administratorDelete.jsp
			</to-view-id>
		</navigation-case>
	</navigation-rule>

	<navigation-rule>
		<display-name>administratorDelete</display-name>
		<from-view-id>
			/stylesheets/administratorDelete.jsp
		</from-view-id>
		<navigation-case>
			<from-outcome>adminDeleted</from-outcome>
			<to-view-id>/stylesheets/administrators.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>cancel</from-outcome>
			<to-view-id>/stylesheets/administrators.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>

	<navigation-rule>
		<display-name>administratorAdd</display-name>
		<from-view-id>/stylesheets/administratorAdd.jsp</from-view-id>
		<navigation-case>
			<from-outcome>adminAdded</from-outcome>
			<to-view-id>/stylesheets/administrators.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>cancel</from-outcome>
			<to-view-id>/stylesheets/administrators.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>ldapSearch</from-outcome>
			<to-view-id>/stylesheets/ldapSearch.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>

	<navigation-rule>
		<display-name>ldapSearch</display-name>
		<from-view-id>/stylesheets/ldapSearch.jsp</from-view-id>
		<navigation-case>
			<from-outcome>usersFound</from-outcome>
			<to-view-id>/stylesheets/ldapResults.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>cancelToAdministratorAdd</from-outcome>
			<to-view-id>/stylesheets/administratorAdd.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>cancelToDepartmentManagerAdd</from-outcome>
			<to-view-id>
				/stylesheets/departmentManagerAdd.jsp
			</to-view-id>
		</navigation-case>
	</navigation-rule>

	<navigation-rule>
		<display-name>ldapResults</display-name>
		<from-view-id>/stylesheets/ldapResults.jsp</from-view-id>
		<navigation-case>
			<from-outcome>userSelectedToAdministratorAdd</from-outcome>
			<to-view-id>/stylesheets/administratorAdd.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>cancelToAdministratorAdd</from-outcome>
			<to-view-id>/stylesheets/administratorAdd.jsp</to-view-id>
			<redirect />
		</navigation-case>
		<navigation-case>
			<from-outcome>
				userSelectedToDepartmentManagerAdd
			</from-outcome>
			<to-view-id>
				/stylesheets/departmentManagerAdd.jsp
			</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>cancelToDepartmentManagerAdd</from-outcome>
			<to-view-id>
				/stylesheets/departmentManagerAdd.jsp
			</to-view-id>
			<redirect />
		</navigation-case>
		<navigation-case>
			<from-outcome>back</from-outcome>
			<to-view-id>/stylesheets/ldapSearch.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>

	<navigation-rule>
		<from-view-id>/stylesheets/departments.jsp</from-view-id>
		<navigation-case>
			<from-outcome>addDepartment</from-outcome>
			<to-view-id>/stylesheets/departmentAdd.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>viewDepartement</from-outcome>
			<to-view-id>/stylesheets/departmentView.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>

	<navigation-rule>
		<from-view-id>/stylesheets/departmentAdd.jsp</from-view-id>
		<navigation-case>
			<from-outcome>departmentAdded</from-outcome>
			<to-view-id>/stylesheets/departmentView.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>cancel</from-outcome>
			<to-view-id>/stylesheets/departments.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>

	<navigation-rule>
		<from-view-id>/stylesheets/departmentView.jsp</from-view-id>
		<navigation-case>
			<from-outcome>editProperties</from-outcome>
			<to-view-id>/stylesheets/departmentEdit.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>addManager</from-outcome>
			<to-view-id>
				/stylesheets/departmentManagerAdd.jsp
			</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>editDepartmentManager</from-outcome>
			<to-view-id>
				/stylesheets/departmentManagerEdit.jsp
			</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>back</from-outcome>
			<to-view-id>/stylesheets/departments.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>deleteDepartment</from-outcome>
			<to-view-id>/stylesheets/departmentDelete.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>

	<navigation-rule>
		<from-view-id>/stylesheets/departmentEdit.jsp</from-view-id>
		<navigation-case>
			<from-outcome>departmentUpdated</from-outcome>
			<to-view-id>/stylesheets/departmentView.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>cancel</from-outcome>
			<to-view-id>/stylesheets/departmentView.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>

	<navigation-rule>
		<from-view-id>/stylesheets/departmentDelete.jsp</from-view-id>
		<navigation-case>
			<from-outcome>departmentDeleted</from-outcome>
			<to-view-id>/stylesheets/departments.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>cancel</from-outcome>
			<to-view-id>/stylesheets/departmentView.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>

	<navigation-rule>
		<from-view-id>
			/stylesheets/departmentManagerAdd.jsp
		</from-view-id>
		<navigation-case>
			<from-outcome>departmentManagerAdded</from-outcome>
			<to-view-id>
				/stylesheets/departmentManagerEdit.jsp
			</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>ldapSearch</from-outcome>
			<to-view-id>/stylesheets/ldapSearch.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>cancel</from-outcome>
			<to-view-id>/stylesheets/departmentView.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>

	<navigation-rule>
		<from-view-id>
			/stylesheets/departmentManagerEdit.jsp
		</from-view-id>
		<navigation-case>
			<from-outcome>departmentManagerUpdated</from-outcome>
			<to-view-id>/stylesheets/departmentView.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>deleteDepartmentManager</from-outcome>
			<to-view-id>
				/stylesheets/departmentManagerDelete.jsp
			</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>cancel</from-outcome>
			<to-view-id>/stylesheets/departmentView.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>

	<navigation-rule>
		<from-view-id>
			/stylesheets/departmentManagerDelete.jsp
		</from-view-id>
		<navigation-case>
			<from-outcome>departmentManagerDeleted</from-outcome>
			<to-view-id>/stylesheets/departmentView.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>cancel</from-outcome>
			<to-view-id>
				/stylesheets/departmentManagerEdit.jsp
			</to-view-id>
		</navigation-case>
	</navigation-rule>

	<navigation-rule>
		<from-view-id>/stylesheets/things.jsp</from-view-id>
		<navigation-case>
			<from-outcome>thingAdded</from-outcome>
			<to-view-id>/stylesheets/thingEdit.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>editThing</from-outcome>
			<to-view-id>/stylesheets/thingEdit.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>

	<navigation-rule>
		<from-view-id>/stylesheets/thingEdit.jsp</from-view-id>
		<navigation-case>
			<from-outcome>thingUpdated</from-outcome>
			<to-view-id>/stylesheets/things.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>cancel</from-outcome>
			<to-view-id>/stylesheets/things.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>deleteThing</from-outcome>
			<to-view-id>/stylesheets/thingDelete.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>

	<navigation-rule>
		<from-view-id>/stylesheets/thingDelete.jsp</from-view-id>
		<navigation-case>
			<from-outcome>thingDeleted</from-outcome>
			<to-view-id>/stylesheets/things.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>cancel</from-outcome>
			<to-view-id>/stylesheets/thingEdit.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>

	<!--navigation-rule>
		<from-view-id>/stylesheets/exception.jsp</from-view-id>
		<navigation-case>
			<from-outcome>applicationRestarted</from-outcome>
			<to-view-id>/stylesheets/welcome.jsp</to-view-id>
		</navigation-case>
	</navigation-rule-->

	<navigation-rule>
		<navigation-case>
			<from-outcome>applicationRestarted</from-outcome>
			<to-view-id>/stylesheets/welcome.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>navigationWelcome</from-outcome>
			<to-view-id>/stylesheets/welcome.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>navigationThings</from-outcome>
			<to-view-id>/stylesheets/things.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>navigationFiles</from-outcome>
			<to-view-id>/stylesheets/files.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>navigationDepartments</from-outcome>
			<to-view-id>/stylesheets/departments.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>navigationDepartmentView</from-outcome>
			<to-view-id>/stylesheets/departmentView.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>navigationAdministrators</from-outcome>
			<to-view-id>/stylesheets/administrators.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>navigationPreferences</from-outcome>
			<to-view-id>/stylesheets/preferences.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>navigationAbout</from-outcome>
			<to-view-id>/stylesheets/about.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>navigationLogin</from-outcome>
			<to-view-id>/stylesheets/login.jsp</to-view-id>
			<redirect />
		</navigation-case>
	</navigation-rule>

</faces-config>
