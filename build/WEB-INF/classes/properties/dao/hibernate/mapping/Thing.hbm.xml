<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
      "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
          "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
          
<hibernate-mapping package="org.esupportail.lecture.domain.beans">
	<class name="Thing" table="d_thin" lazy="false">
	<cache usage="nonstrict-read-write"/>
	<id name="id">
			<generator class="native"/>
		</id>
	<property name="value" column="valu" />
	<property name="comment" column="comm" />
	<many-to-one name="department" column="depa_id" not-null="true" fetch="join" /> <!-- JOIN-->
	<many-to-one name="user" column="user_id" fetch="join" /> <!-- JOIN-->
	<property name="date" column="dat" />
	<property name="order" column="orde" />
	</class>	
</hibernate-mapping>


