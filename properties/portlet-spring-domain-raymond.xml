<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN//EN" "http://www.springframework.org/dtd/spring-beans.dtd">

<beans>

	<bean id="categorie1" class="org.esupportail.lecture.domain.model.tmp.CategoryRB">
		<property name="id" value="1" />
		<property name="name" value="Bibliothèques" />
		<property name="selected" value="false" />
		<property name="expanded" value="false" />
		<property name="sources">
			<list>
				<ref bean="source3" />
			</list>
		</property>
	</bean>
	<bean id="categorie2" class="org.esupportail.lecture.domain.model.tmp.CategoryRB">
		<property name="id" value="2" />
		<property name="name" value="Vie culturelle" />
		<property name="selected" value="true" />
		<property name="expanded" value="true" />
		<property name="sources">
			<list>
