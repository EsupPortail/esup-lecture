		<property name="expanded" value="false" />
		<property name="sources">
			<list>
				<ref bean="source4" />
			</list>
		</property>
	</bean>
	
	<bean id="source1"
		class="org.esupportail.lecture.domain.model.tmp.SourceRB">
		<property name="id" value="1" />
		<property name="name" value="Cinéma" />
		<property name="selected" value="true" />
