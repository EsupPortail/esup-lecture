<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="org.esupportail.lecture.domain.model">
	<class name="UserProfile" table="USERPROFILE">
		<id name="userProfilePK" type="long" column="USERPROFILEPK">
			<generator class="native" />
		</id>
		<property name="userId" type="string" column="USERID">
		</property>
		<map name="customContexts" lazy="true" cascade="all">
			<key column="USERPROFILE_USERPROFILEPK" not-null="true"/>
			<index column="CONTEXTID" type="string"/>
			<one-to-many class="CustomContext" />
		</map>
		<map name="customCategories" cascade="all">
			<key column="USERPROFILE_USERPROFILEPK" not-null="true"/>
			<map-key column="CUSTOMCATEGORYID" type="string"/>
			<one-to-many class="CustomManagedCategory" />
		</map>
	</class>
</hibernate-mapping>